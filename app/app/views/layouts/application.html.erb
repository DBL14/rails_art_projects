<!DOCTYPE html>
<html>
<head>
  <title>App</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<% if current_user %>

		<p><%= current_user.email %> | <%= link_to 'sign out', sessions_path, method: :delete %></p>

	<% end %>



	<%= yield %>

	
</body>
</html>
