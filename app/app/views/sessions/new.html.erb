<h3>Login</h3>
<%= form_tag sessions_path do %>
	<div class="field">
		<%= label_tag :email %>
		<%= text_field_tag :email %>
	</div>
	<div class="field">
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	</div>
	<div class="actions"><%= submit_tag "log in" %></div>
<% end %>

<p id="sign up"><%= link_to "sign up", new_user_path, method: :get %></p>



